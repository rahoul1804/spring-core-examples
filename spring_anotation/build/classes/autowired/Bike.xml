<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
   
 <context:annotation-config/>
 
 <bean id="bd" class="autowired.Bike_specification" >
 <property name="modelNumber" value="1234"></property>
 <property name="noOfGears" value="4"></property>
 <property name="price" value="150000"></property>
 </bean>
 
 <bean id="b" class="autowired.Bike">
 <property name="bikeNumber" value="5864"></property>
 <property name="BikeName" value="Hero"></property>
 </bean>
 
 </beans>